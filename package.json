{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"test:ui": "playwright test --ui",
		"test:debug": "playwright test --debug",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint --fix",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@playwright/test": "^1.33.0",
		"@sveltejs/kit": "^1.16.0",
		"@threlte/core": "6.0.0-next.6",
		"@threlte/extras": "5.0.0-next.11",
		"@threlte/preprocess": "^0.0.2",
		"@threlte/rapier": "1.0.0-next.2",
		"@types/set-cookie-parser": "^2.4.2",
		"@types/three": "^0.151.0",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.59.2",
		"@typescript-eslint/parser": "^5.59.2",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-svelte": "^2.27.3",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"postcss": "^8.4.23",
		"postprocessing": "^6.30.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.3.0",
		"svelte-sequential-preprocessor": "^1.0.0",
		"sveltekit-superforms": "^0.8.5",
		"tailwindcss": "^3.3.2",
		"three": "^0.151.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"uuid": "^9.0.0",
		"vite": "^4.3.4"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.2.4",
		"@zodios/core": "^10.9.0",
		"set-cookie-parser": "^2.6.0",
